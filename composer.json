{
    "name": "theroadbunch/csvmachine",
    "description": "A Delimiter-separated values manipulation library",
    "keywords": [ "csv", "dsv", "comma", "delimiter" ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "dan",
            "email": "danmcadams@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RoadBunch\\Csv\\": "src/Csv"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RoadBunch\\Csv\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "psr/log": "^1.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "symfony/var-dumper": "^3.4",
        "monolog/monolog": "^1.23",
        "fzaninotto/faker": "^1.7"
    },
    "suggest": {
        "monolog/monolog": "Allows for in-depth logging of the csv machining."
    }
}
